{
  "name": "@mutuals/payload",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./src/*.ts",
    "./access": "./src/access/index.ts",
    "./blocks/*": "./src/blocks/*/index.ts",
    "./collections": "./src/collections/index.ts",
    "./fields": "./src/fields/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./utils": "./src/utils/index.ts",
    "./payload-types": "./src/payload-types.ts",
    "./payload-config/*": "./src/payload.config.ts"
  },
  "scripts": {
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "eslint . --max-warnings=0",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "peerDependencies": {
    "@payloadcms/db-postgres": ">=3",
    "@payloadcms/next": ">=3",
    "graphql": ">=16",
    "next": ">=15",
    "payload": ">=3"
  },
  "dependencies": {
    "@payloadcms/plugin-redirects": "^3.69.0",
    "@payloadcms/plugin-seo": "^3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/storage-vercel-blob": "^3.69.0",
    "@payloadcms/ui": "3.69.0",
    "cross-env": "^10.1.0",
    "dotenv": "17.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@mutuals/eslint-config": "workspace:*",
    "@mutuals/typescript-config": "workspace:*",
    "@playwright/test": "1.57.0",
    "@testing-library/react": "16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "^9.39.2",
    "jsdom": "27.4.0",
    "playwright": "1.57.0",
    "playwright-core": "1.57.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
}
